#menu ul { /* vamos estilizar uma lista na div menu */
    margin:0px 0px 0px 20px; /* margem no sentido horário(topo,direita,base,esquerda) */
    padding:0px; /* sem espaçamento */
    list-style-type:none; /* tipo do estilo da lista */
   /* border-left:0px solid #DDD;  definimos os atributos da borda esquerda */
     ;
}
     
#menu li { /* vamos estilizar os elementos da lista na div menu */

    padding:0px 10px 0px 10px; /* espaçamento no sentido horário(topo,direita,base,esquerda) */
    border-right:0px solid #DDD; /* definimos os atributos da borda direita */
    
}
     
#menu li a { /* link no estado normal e visitado */
    text-decoration:none; /* sem decoração no link */
    color:color:#070589; /* definimos a cor do link */
}
     
#menu li a:hover { /* link no estado quando se passa o mouse em cima */
    text-decoration:; /* decoração sublinhado no link */
    background-color:#990; /* cor de fundo no link */
}

#box_center {
    float:	; /* definimos que a div lateral vai flutuar para a esquerda */
    padding:0px; /* espaçamento de 4px para todos os lados */
    border:2px outset #AAA; /* definimos os atributos da borda */
    width:230px; /* Temos 190px + 2px da borda direita e esquerda + 8px do espaçamento = 200px */
    border-radius: 10px;       
    margin:3  3px ;
    position:relative; /* definimos que a nossa div tem posição relativa */
    left: 130px; /* com isso definimos que ela vai ter margem de 5px da direita */
   background-color:
}

